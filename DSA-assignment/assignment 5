#include <stdio.h>
#define MAX 10

int graph[MAX][MAX];
int n;

void addEdge(int u,int v){
    graph[u][v]=1;
    graph[v][u]=1;
}

void display(){
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++)
            printf("%d ",graph[i][j]);
        printf("\n");
    }
}

int main(){
    n=5;
    addEdge(0,1);
    addEdge(0,2);
    addEdge(1,3);
    addEdge(2,4);

    printf("Adjacency Matrix:\n");
    display();
    return 0;
}